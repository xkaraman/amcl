<!-- Turtlebot Move_base.launch.xml default -->
<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
		<rosparam file="$(find amcl_depth)/launch/param/costmap_common_params.yaml"
									command="load" ns="global_costmap" />
		<rosparam file="$(find amcl_depth)/launch/param/costmap_common_params.yaml"
									command="load" ns="local_costmap" />
		<rosparam file="$(find amcl_depth)/launch/param/local_costmap_params.yaml"
									command="load" />
		<rosparam file="$(find amcl_depth)/launch/param/global_costmap_params.yaml"
									command="load" />
   		<rosparam file="$(find amcl_depth)/launch/param/dwa_local_planner_params.yaml" 										command="load" />					
   		<rosparam file="$(find amcl_depth)/launch/param/move_base_params.yaml" 
   									command="load" />
   		<rosparam file="$(find amcl_depth)/launch/param/global_planner_params.yaml" 
   									command="load" />		
    	<rosparam file="$(find amcl_depth)/launch/param/navfn_global_planner_params.yaml" 
    								command="load"/>	
	</node>
</launch>
